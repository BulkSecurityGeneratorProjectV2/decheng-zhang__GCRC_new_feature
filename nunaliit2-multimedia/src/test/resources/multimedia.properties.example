;ffmpegInfoCommand=ffmpeg -i %1$s
;ffmpegConvertVideoCommand=ffmpeg -i %1$s -y -acodec libfaac -ab 48000 -ac 2 -vcodec libx264 -vpre hq -b 128000 -s 320x240 -threads 0 -f mp4 %2$s
;ffmpegConvertAudioCommand=ffmpeg -i %1$s -y -acodec libmp3lame -ab 48000 -ac 2 -threads 0 -f mp3 %2$s
;ffmpegCreateThumbnailCommand=ffmpeg -y -ss 00:00:05 -i %1$s -s %3$dx%4$d -r 1 -vframes 1 -f image2 %2$s
;imageInfoCommand=identify -verbose %1$s[0]
;imageConvertCommand=convert -monitor -auto-orient %1$s[0] -compress JPEG -quality 70 %2$s
;imageResizeCommand=convert -monitor -auto-orient %1$s[0] -resize %3$dx%4$d -compress JPEG -quality 70 %2$s
;imageReorientCommand=convert -monitor -auto-orient %1$s[0] %2$s
;thumbnailImageHeight=60
;thumbnailImageWidth=60
;thumbnailVideoHeight=60
;thumbnailVideoWidth=80
;imageMimeTypes=image/png:image/jpeg
;audioMimeTypes=application/ogg
;videoMimeTypes=
;multimedia.conversion.audio.threshold=mpeg,250000
;multimedia.conversion.video.threshold=h264,250000,mpeg4aac,250000,*
;multimedia.conversion.image.threshold=JPEG,500
